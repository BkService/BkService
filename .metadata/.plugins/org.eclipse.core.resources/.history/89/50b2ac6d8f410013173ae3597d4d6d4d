package junior.server.core.data.events;

//import java.sql.Time;
import java.util.LinkedList;

import junior.server.core.data.markets.*;

public class Event {
	LinkedList<Market> markets; // контейнер с маркетами
	String description; // описание события
//	Time startTime;	// время начала
//	Time finishTime;	// время окончания
	
	public Event(){
		markets = new LinkedList<Market>();
		description = "No description available.";
	}
	
	public Event(String new_description){
		description = new_description;
	}
	
	public Event(LinkedList<Market> markets, String description)
}
